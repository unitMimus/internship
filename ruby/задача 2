def  coincidence(array=[], range=())
  if array.length>0 && range.to_a.length>0
    print "#{array.keep_if{|i| range.include?(i)}}\n"
  else  print "#{[]}\n"
  end
end


coincidence([1, 2, 3, 4, 5], (3..5))  # => [3, 4, 5] 
coincidence()  # => [] 
coincidence([nil, 1, 'foo', 4, 2, 2.5], (1..3)) # => [1, 2, 2.5] 
